{
  "name": "techtestapp",
  "version": "2.1.27",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-dev": "react-native run-android --variant=devDebug",
    "android-stg": "react-native run-android --variant=stgDebug",
    "android-prod": "react-native run-android --variant=prodDebug",
    "run-android": "yarn lint && yarn android",
    "run-android-dev": "yarn lint && yarn android-dev",
    "run-android-stg": "yarn lint && yarn android-stg",
    "run-android-prod": "yarn lint && yarn android-prod",
    "uninstall-android-release": "adb shell pm uninstall com.mobilite.techtest",
    "uninstall-android-dev-release": "adb shell pm uninstall com.mobilite.techtest.dev",
    "uninstall-android-stg-release": "adb shell pm uninstall com.mobilite.techtest.stg",
    "uninstall-android-prod-release": "adb shell pm uninstall com.mobilite.techtest",
    "android-release": "yarn uninstall-android-release & react-native run-android --variant=release",
    "android-dev-release": "yarn uninstall-android-dev-release & react-native run-android --variant=devRelease",
    "android-stg-release": "yarn uninstall-android-stg-release & react-native run-android --variant=stgRelease",
    "android-prod-release": "yarn uninstall-android-prod-release & react-native run-android --variant=prodRelease",
    "run-android-release": "yarn lint && yarn android-release",
    "run-android-dev-release": "yarn lint && yarn android-dev-release",
    "run-android-stg-release": "yarn lint && yarn android-stg-release",
    "run-android-prod-release": "yarn lint && yarn android-prod-release",
    "run-android-dev-assemble": "yarn lint && cd android && ./gradlew --stacktrace assembleDevRelease",
    "run-android-stg-assemble": "yarn lint && cd android && ./gradlew --stacktrace assembleStgRelease",
    "run-android-prod-assemble": "yarn lint && cd android && ./gradlew --stacktrace assembleProdRelease",
    "run-android-production-bundle": "yarn lint && cd android && ./gradlew --stacktrace bundleProdRelease",
    "ios": "react-native run-ios",
    "ios-dev": "react-native run-ios —-scheme TechtestAppDev —-configuration DebugDev",
    "ios-stg": "react-native run-ios —-scheme TechtestAppStg —-configuration DebugStg",
    "ios-prod": "react-native run-ios —-scheme TechtestAppProd —-configuration DebugProd",
    "run-ios": "yarn lint && yarn ios",
    "run-ios-dev": "yarn lint && yarn i os-dev",
    "run-ios-stg": "yarn lint && yarn ios-stg",
    "run-ios-prod": "yarn lint && yarn ios-prod",
    "ios-release": "react-native run-ios --configuration Release",
    "ios-dev-release": "react-native run-ios —-scheme TechtestAppDev —-configuration ReleaseDev",
    "ios-stg-release": "react-native run-ios —-scheme TechtestAppStg —-configuration ReleaseStg",
    "ios-prod-release": "react-native run-ios —-scheme TechtestAppProd —-configuration ReleaseProd",
    "run-ios-release": "yarn lint && yarn ios-release",
    "run-ios-dev-release": "yarn lint && yarn ios-dev-release",
    "run-ios-stg-release": "yarn lint && yarn ios-stg-release",
    "run-ios-prod-release": "yarn lint && yarn ios-prod-release",
    "start": "react-native start",
    "test": "jest",
    "prettier:write": "npx prettier --write \"**/*.{js,jsx,ts,tsx,json}\" && npx prettier --write \"*.{js,jsx,ts,tsx,json}\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install"
  },
